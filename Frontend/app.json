{
  "expo": {
    "name": "City Forge",
    "scheme": "Aura",
    "slug": "Construction Management App for SIH2024",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mini.png",
      "resizeMode": "contain",
      "backgroundColor": "#1d6b6b"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mini.png",
        "backgroundColor": "#1d6b6b"
      }
    },
    "web": {
      "mini": "./assets/mini.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
